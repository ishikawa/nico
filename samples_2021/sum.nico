# i32[] -> i32
fun sum(numbers: Int[])
    case numbers
    when []
        0
    when [x, ...xs]
        x + sum(xs)
    end
end

println_i32(sum([])) # => 0
println_i32(sum([409, 221, 56, 16, 151, 31, 198, 211, 231, 142])) # => 1666
println_i32(sum([-137, -21, 443, 349, -495, -141, -138, -495, -168, -412])) # => -1215
println_i32(sum([sum([200, 360, 223, 176, 127]), sum([462, 430, 65, 244, 82])])) # => 2369